#include<iostream>
using namespace std;
int dp[41]={0,1,1};
int main(){
    int n,num;
    cin>>n;
    ios::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);
 
    for(int i=3; i<41; i++){
       dp[i]=dp[i-2]+dp[i-1];
    }
    for( int i=0;i<n; i++){
        cin>>num;
        if(num==0){
            cout<<1<<" "<<0<<"\n";
        }
        else if( num==1){
            cout<<0<<" "<<1<<"\n";
        }
        else{
            cout<<dp[num-1]<<" "<<dp[num]<<"\n";
        }
     
    }
   
    
}
